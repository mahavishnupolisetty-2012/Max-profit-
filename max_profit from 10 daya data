#include <stdio.h>
int main()
{
    int i,a[10],min,max;
    for(i=0;i<9;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    for(i=1;i<9;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    max=a[0];
    for(i=1;i<9;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    if((1000-min)>(max-1000))
    {
        printf("The profit is %d\n",(1000-min));
    }
    else
    {
        printf("The profit is %d\n",(max-1000));
    }

    return 0;
}
